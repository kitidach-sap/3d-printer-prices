{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        },
        {
            "src": "api/cron/scrape.js",
            "use": "@vercel/node"
        },
        {
            "src": "api/cron/blog.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/cron/scrape",
            "dest": "api/cron/scrape.js"
        },
        {
            "src": "/api/cron/blog",
            "dest": "api/cron/blog.js"
        },
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ],
    "crons": [
        {
            "path": "/api/cron/scrape",
            "schedule": "0 6 * * *"
        },
        {
            "path": "/api/cron/blog",
            "schedule": "0 8 * * 1"
        }
    ]
}